{% block html_head_container %}

    {% include 'html-header.twig' %}
    {% block head %}{% endblock %}
</head>{% endblock %}<body class="{{body_class}}" data-template="base.twig">
<div style="display:inline">{% include '/svg/svg.svg' %}</div>
<a class="skip-link screen-reader-text" href="#content">{{ _e( 'Skip to content') }}</a>
<header class="header">
    {% block header %}
        <div class="wrapper">
            <div class="row grid">
                <h1 class="column text-left hdr-logo" role="banner">
                    <a class="hdr-logo-link" href="{{site.url}}" rel="home">{{site.name}}</a>
                </h1>
                <div class="column"></div>
                <div class="column">
                    <ul class="navbar">
                        <li><a href="#">My Student's Progress</a></li>
                        <li><a href="#">Smart Tuition</a></li>
                    </ul>
                </div>
            </div>
            <nav class="nav-main" id="nav-main" role="navigation">
                {% include "menu.twig" with {'menu': menu.get_items} %}
            </nav>
            <!-- #nav -->
        </div>
    {% endblock %}
</header>

<section class="content-wrapper" id="content" role="main">
    {% if title %}
        <h1>{{title}}</h1>
    {% endif %}
    <div class="wrapper {{sidebar_class}}">
        {% block content %}
            Sorry, no content
        {% endblock %}
    </div>
    {% if sidebar %}
        <aside class="layout-sidebar">
            {{sidebar}}
        </aside>
    {% endif %}
</section>

{% block footer %}
    <footer id="footer">
        {% include 'footer.twig' %}
    </footer>
    {{ function('wp_footer') }}
{% endblock %}</body></html>
